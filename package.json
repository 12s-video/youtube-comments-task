{
  "name": "rx-youtube-comments",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "watch": {
    "unit-test": {
      "patterns": [
        "./",
        "test"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "scripts": {
    "test": "npm run unit-test && npm run integration-test",
    "unit-test": "mocha test/*.js test/lib/*.js test/lib/**/*.js",
    "integration-test": "mocha test/integration-tests/*.js",
    "coverage": "nyc --reporter=html mocha test/*.js test/lib/*.js test/lib/**/*.js && xdg-open ./coverage/index.html",
    "format": "standard-format --write",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philbot9/rx-youtube-comments.git"
  },
  "keywords": [
    "youtube",
    "comments",
    "stream",
    "rx",
    "rxjs",
    "reactive"
  ],
  "author": "Philip Klostermann <philip.klostermann@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/philbot9/rx-youtube-comments/issues"
  },
  "homepage": "https://github.com/philbot9/rx-youtube-comments#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "control.monads": "^0.6.0",
    "core.lambda": "^1.0.0",
    "data.either": "^1.4.0",
    "data.task": "^3.1.1",
    "debug": "^2.2.0",
    "immutable": "^3.8.1",
    "immutable-ext": "^1.0.8",
    "moment": "^2.17.1",
    "promised-until": "^1.0.2",
    "propper": "^1.0.2",
    "request": "^2.74.0",
    "rxjs": "^5.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "npm-watch": "^0.1.6",
    "nyc": "^10.1.2",
    "standard": "^8.6.0",
    "standard-format": "^2.2.4",
    "testdouble": "^1.10.1"
  }
}
